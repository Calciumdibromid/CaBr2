        {
          "drnr": "0614",
          "text": "<ueberschrift>\n            <span name=\"0614\">pH-WERT</span>\n         </ueberschrift>\n         <table class=\"leerzeile\">\n            <th height=\"10\" />\n         </table>\n         <table class=\"block\">\n            <colgroup>\n               <col width=\"100%\" />\n            </colgroup>\n            <tr valign=\"top\">\n               <td align=\"left\">pH-Wert:</td>\n            </tr>\n            <tr valign=\"top\">\n               <td align=\"left\">alkalisch</td>\n            </tr>\n         </table>\n         <table class=\"block\">\n            <colgroup>\n               <col width=\"10%\" />\n               <col width=\"90%\" />\n            </colgroup>\n            <tr valign=\"top\">\n               <td align=\"left\">Quelle:</td>\n               <td align=\"left\">\n                  <a href=\"#08005\">08005</a> </td>\n            </tr>\n         </table>\n      "
