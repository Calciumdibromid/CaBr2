{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "dependencyDashboard": true,
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchPackagePatterns": ["js"],
      "labels": ["typescript"]
    },
    {
      "matchPackagePatterns": ["rust"],
      "labels": ["rust"]
    }
  ]
}
